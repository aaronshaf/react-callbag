{
  "name": "react-callbag",
  "version": "2.0.2",
  "description": "Callbag-related React components",
  "main": "dist/index.js",
  "files": [
    "dist/index.js"
  ],
  "scripts": {
    "start": "parcel example/index.html -d docs",
    "build-component": "babel src --out-dir dist",
    "build": "mkdir -p dist && rm -df dist/* && npm run build-component",
    "build-docs": "parcel build example/index.html -d docs --public-url ./",
    "test": "nyc mocha --require babel-register --require spec/setup.js spec/**/*.spec.js",
    "lint": "eslint .; exit 0"
  },
  "keywords": [
    "component",
    "react",
    "react-component"
  ],
  "author": "Aaron Shafovaloff <aaronshaf@gmail.com>",
  "homepage": "https://github.com/aaronshaf/react-callbag",
  "repository": {
    "type": "git",
    "url": "git@github.com:aaronshaf/react-callbag.git"
  },
  "bugs": {
    "url": "https://github.com/aaronshaf/react-callbag/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.16.3",
    "callbag-debounce": "^2.0.0",
    "callbag-map": "^1.0.1",
    "callbag-skip": "^1.0.0",
    "callbag-start-with": "^1.0.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.2",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.7.1",
    "eslint-config-standard": "^6.2.0",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.3.0",
    "eslint-plugin-standard": "^2.0.1",
    "flow": "^0.2.3",
    "jsdom": "^9.6.0",
    "mocha": "^3.1.2",
    "nyc": "^11.0.2",
    "parcel-bundler": "^1.6.2",
    "prop-types": "^15.3.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  },
  "dependencies": {
    "callbag-pipe": "^1.1.1",
    "callbag-subject": "^1.0.2",
    "callbag-subscribe": "^1.2.0"
  },
  "peerDependencies": {
    "prop-types": "^15.3.0",
    "react": "^15.3.0 || ^16.0.0",
    "react-dom": "^15.3.0 || ^16.0.0"
  }
}
